class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int n = nums.size();
        unordered_map<int,int> mpp;
        mpp[0] = 1;
        int cnt = 0;
        int ans = 0;
        for(int i = 0; i < n;i++){
            cnt += nums[i];
            ans += mpp[cnt - k];
            mpp[cnt]++;
        }
        return ans;
    }
};
