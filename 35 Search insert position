class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int n = nums.size();
        int l = 0;
        int r = n - 1;
        int ans = 0;

        while(l <= r){
            int mid = (r - l)/2 + l;
            if(nums[mid] == target) return mid;
            if(target < nums[mid]) r = mid - 1;
            else l = mid + 1;
        }
        return l;
    }
};
